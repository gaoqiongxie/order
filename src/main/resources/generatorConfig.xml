<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
 PUBLIC " -//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry
		location="/D:/mvn/repository/mysql/mysql-connector-java/5.1.30/mysql-connector-java-5.1.30.jar" />
	
	<context id="XHTables" targetRuntime="MyBatis3">
		<commentGenerator type="generator.MyCommentGenerator">
			<!-- <property name="suppressDate" value="false" />
			注释
			<property name="suppressAllComments" value="true" />
			是否显示数据库字段名备注
            <property name="addRemarkComments" value="false" /> -->
		</commentGenerator>
		
		<!-- mysql数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.16.85.126:3306/xw" userId="ydhx"
			password="123456" />
			
		<javaTypeResolver type="generator.MyJavaTypeResolver">
		    <property name="forceBigDecimals" value="false"></property>
		</javaTypeResolver>
		
		<!-- 生成model实体类文件位置 -->
		<javaModelGenerator
			targetPackage="com.order.entity"
			targetProject="/D:/xw_git/order/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 生成mapper.xml配置文件位置 -->
		<sqlMapGenerator
			targetPackage="mybatis/mapper"
			targetProject="/D:/xw_git/order/src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 生成mapper接口文件位置 -->
		<javaClientGenerator
			targetPackage="com.order.dao"
			targetProject="/D:/xw_git/order/src/main/java"
			type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 需要生成的实体类对应的表名，多个实体类复制多份该配置即可 -->
		<table tableName="order_detail" domainObjectName="OrderDetail"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>